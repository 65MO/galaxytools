<tool id="CONVERTER_sdf_to_smiles" name="SDF to SMILES" version="1.0.0">
    <description></description>
    <command >obabel -isdf $input -osmi -O $output -e 2>&#38;1</command>
    <inputs>
        <param name="input" type="data" format="sdf" label="Molecules in SD-format"/>
    </inputs>
    <outputs>
        <data name="output" format="smi"/>
    </outputs>
    <requirements>
        <requirement type="binary">obabel</requirement>
    </requirements>
    <help>
    </help>
</tool>
