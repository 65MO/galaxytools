<tool id="locarna" name="locarna" version="0.1.0">
	<description></description>
	<requirements>
	<requirement type="package" version="x.y.z">locarna</requirement>
	</requirements>
	<command>locarna $locarna $LocARNA $Usage: $Options: $Scoring $match $mismatch $ribosumfile=f(RIBOSUM85_60) $useribosum $indel $indelopening $structweight $expprob $tau $exclusion $stacking $Type $structlocal $sequlocal $freeendgaps $normalized $Controlling $width $clustal=file $pp=file $alifoldconsensusdp $L,localoutput $localfileoutput $P,posoutput $writestructure $scorecomponents $stopwatch $Heuristics $minprob $maxdiffam $maxdiff $maxdiffaln $maxdiffpwaln $maxdiffrelax $minamprob $minbmprob $Special $kbest $better $Options $meaalignment $probconsfile=file $matchprobmethod $temperature $pfstructweight $meagapcost $meaalpha $meabeta $meagamma $probabilityscale $writematchprobs=file $readmatchprobs=file $writearcmatchscores=file $readarcmatchscores=file $readarcmatchprobs=file $Constraints: $noLP $ignoreconstraints $RNA $file1 $file2 $Report 
	</command>
	<inputs>
	<param name="match" label="Match score " type="integer" value="50" help="" />
	<param name="mismatch" label="Mismatch score " type="integer" value="0" help="" />
	<param format="txt" name="ribosumfile=f(RIBOSUM85_60)" type="data" label="Ribosum file " />
	<param name="useribosum" type="boolean" checked="false" truevalue="--use-ribosum" falsevalue="" label="Use ribosum scores " help="" />
	<param name="indel" label="Indel score " type="integer" value="-350" help="" />
	<param name="indelopening" label="Indel opening score " type="integer" value="-500" help="" />
	<param name="structweight" label="Maximal weight of 1/2 arc match " type="integer" value="200" help="" />
	<param name="expprob" label="Expected probability " type="integer" value="1" help="" />
	<param name="tau" label="Tau factor in percent " type="integer" value="0" help="" />
	<param name="exclusion" label="Exclusion weight " type="integer" value="0" help="" />
	<param name="stacking" type="boolean" checked="false" truevalue="--stacking" falsevalue="" label="Use stacking terms (needs stack-probs by RNAfold -p2) " help="" />
	<param name="structlocal" type="boolean" checked="false" truevalue="--struct-local" falsevalue="" label="Structure local " help="" />
	<param name="sequlocal" type="boolean" checked="false" truevalue="--sequ-local" falsevalue="" label="Sequence local " help="" />
	<param name="normalized" label="Normalized local alignment with parameter L " type="integer" value="0" help="" />
	<param name="width" label="Output width " type="integer" value="120" help="" />
	<param format="txt" name="clustal=file" type="data" label="Clustal output " />
	<param format="txt" name="pp=file" type="data" label="PP output " />
	<param name="alifoldconsensusdp" type="boolean" checked="false" truevalue="--alifold-consensus-dp" falsevalue="" label="Compute consensus dot plot by alifold " help="" />
	<param name="L,localoutput" type="boolean" checked="false" truevalue="-L,--local-output" falsevalue="" label="Output only local sub-alignment (to standard out) " help="" />
	<param format="txt" name="localfileoutput" type="data" label="Output only local sub-alignment to pp and " />
	<param name="P,posoutput" type="boolean" checked="false" truevalue="-P,--pos-output" falsevalue="" label="Output only local sub-alignment positions " help="" />
	<param name="writestructure" type="boolean" checked="false" truevalue="--write-structure" falsevalue="" label="Write guidance structure in output " help="" />
	<param name="scorecomponents" type="boolean" checked="false" truevalue="--score-components" falsevalue="" label="Output components of the score (experimental) " help="" />
	<param name="stopwatch" type="boolean" checked="false" truevalue="--stopwatch" falsevalue="" label="Print run time information. " help="" />
	<param name="minprob" label="Minimal probability " type="float" value="0.0005" help="" />
	<param name="maxdiffam" label="Maximal difference for sizes of matched arcs " type="integer" value="-1" help="" />
	<param name="maxdiff" label="Maximal difference for alignment traces " type="integer" value="-1" help="" />
	<param name="maxdiffrelax" type="boolean" checked="false" truevalue="--max-diff-relax" falsevalue="" label="Relax deviation constraints in multiple aligmnent " help="" />
	<param name="minamprob" label="Minimal Arc-match probability " type="float" value="0.0005" help="" />
	<param name="minbmprob" label="Minimal Base-match probability " type="float" value="0.0005" help="" />
	<param name="kbest" label="Enumerate k-best alignments " type="integer" value="-1" help="" />
	<param name="better" label="Enumerate alignments better threshold t " type="integer" value="-1000000" help="" />
	<param name="meaalignment" type="boolean" checked="false" truevalue="--mea-alignment" falsevalue="" label="Do MEA alignment " help="" />
	<param format="txt" name="probconsfile=file" type="data" label="Probcons parameter file " />
	<param name="matchprobmethod" label="Method for computation of match probs " type="integer" value="0" help="" />
	<param name="temperature" label="Temperature for PF-computation " type="integer" value="150" help="" />
	<param name="pfstructweight" label="Structure weight in PF-computation " type="integer" value="200" help="" />
	<param name="meagapcost" type="boolean" checked="false" truevalue="--mea-gapcost" falsevalue="" label="Use gap cost in mea alignment " help="" />
	<param name="meaalpha" label="Weight alpha for MEA " type="integer" value="0" help="" />
	<param name="meabeta" label="Weight beta for MEA " type="integer" value="200" help="" />
	<param name="meagamma" label="Weight gamma for MEA " type="integer" value="100" help="" />
	<param name="probabilityscale" label="Scale for probabilities/resolution of mea score " type="integer" value="10000" help="" />
	<param format="txt" name="writematchprobs=file" type="data" label="Write match probs to file (don't " />
	<param format="txt" name="readmatchprobs=file" type="data" label="Read match probabilities from file " />
	<param format="txt" name="writearcmatchscores=file" type="data" label="Write arcmatch scores (don't align!) " />
	<param format="txt" name="readarcmatchscores=file" type="data" label="Read arcmatch scores " />
	<param format="txt" name="readarcmatchprobs=file" type="data" label="Read arcmatch probabilities (weight by mea_beta/100) " />
	<param name="noLP" type="boolean" checked="false" truevalue="--noLP" falsevalue="" label="No lonely pairs " help="" />
	<param name="ignoreconstraints" type="boolean" checked="false" truevalue="--ignore-constraints" falsevalue="" label="Ignore constraints input files " help="" />
	<param format="txt" name="file1" type="data" label="1> Basepairs input file " />
	<param format="txt" name="file2" type="data" label="2> Basepairs input file " />
	</inputs>
	<outputs>
	<data name="output" format="input" metadata_source="input" /></outputs>
	<help></help></tool>
